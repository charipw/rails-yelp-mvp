<%= form_with model: restaurant do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %> <br>
  <%= f.label :address %>
  <%= f.text_field :address %> <br>
  <%= f.label :phone_number %>
  <%= f.text_field :phone_number %> <br>
  <%= f.label :category, collection: Restaurant::CATEGORY  %>
  <%= f.text_field :category, collection: Restaurant::CATEGORY %><br>
  <%= f.submit "Add new restaurant", class: "btn-btn-primary" %>
<% end %>
